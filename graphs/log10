digraph ortho {
	graph [nodesep=0.8 rankdir=LR splines=ortho]
	node [shape=Mrecord]
	edge [penwidth=2]
	"XORs a->['c', 'd', 'b']" [label="Ã—" fixedsize=true fontsize=35 height=.6 shape=diamond width=.6]
	a -> "XORs a->['c', 'd', 'b']"
	"XORs a->['c', 'd', 'b']" -> c
	"XORs a->['c', 'd', 'b']" -> d
	"XORs a->['c', 'd', 'b']" -> b
	"XORm ['b', 'c']->['d', 'b', 'c']" [label="Ã—" fixedsize=true fontsize=35 height=.6 shape=diamond width=.6]
	"XORm ['b', 'c']->['d', 'b', 'c']" -> d
	"XORm ['b', 'c']->['d', 'b', 'c']" -> b
	"XORm ['b', 'c']->['d', 'b', 'c']" -> c
	b -> "XORm ['b', 'c']->['d', 'b', 'c']"
	c -> "XORm ['b', 'c']->['d', 'b', 'c']"
	start [label="" shape=circle]
	start -> a
	end [label="" shape=circle]
	d -> end
}
