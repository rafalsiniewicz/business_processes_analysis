	edge [penwidth=2]
	a -> b
	"XORs b->['c', 'b']" [label="Ã—" fixedsize=true fontsize=35 height=.6 shape=diamond width=.6]
	b -> "XORs b->['c', 'b']"
	"XORs b->['c', 'b']" -> c
	"XORs b->['c', 'b']" -> b
