digraph ortho {
	graph [nodesep=0.8 rankdir=LR splines=ortho]
	node [shape=Mrecord]
	edge [penwidth=2]
	a -> b
	b -> c
	c -> d
	start [label="" shape=circle]
	"XORs start->{'a', 'b'}" [label="Ã—" fixedsize=true fontsize=35 height=.6 shape=diamond width=.6]
	start -> "XORs start->{'a', 'b'}"
	"XORs start->{'a', 'b'}" -> a
	"XORs start->{'a', 'b'}" -> b
	end [label="" shape=circle]
	"XORm {'d', 'c'}->end" [label="Ã—" fixedsize=true fontsize=35 height=.6 shape=diamond width=.6]
	"XORm {'d', 'c'}->end" -> end
	d -> "XORm {'d', 'c'}->end"
	c -> "XORm {'d', 'c'}->end"
}
